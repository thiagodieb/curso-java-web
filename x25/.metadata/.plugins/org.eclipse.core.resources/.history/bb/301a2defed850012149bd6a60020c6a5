package actions;

import com.opensymphony.xwork2.ActionSupport;

import beans.Curso;

public class CursoAction extends ActionSupport{
	
	private Curso curso;
	
	public Curso getCurso() {
		return curso;
	}

	public void setCurso(Curso curso) {
		this.curso = curso;
	}

	public String add(){
		System.out.println(this.getCurso());
		return "ok";
	}
	
	public String execute(){
		return "success";
	}

}
