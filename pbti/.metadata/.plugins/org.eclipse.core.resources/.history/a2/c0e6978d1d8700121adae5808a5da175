package interceptores;import javax.servlet.http.HttpServletRequest;import javax.servlet.http.HttpSession;import org.apache.struts2.ServletActionContext;import com.opensymphony.xwork2.*;import com.opensymphony.xwork2.interceptor.Interceptor;public class LoginInterceptor implements Interceptor {	public void destroy() {	}	public void init() {	}	public String intercept(ActionInvocation invocation) throws Exception {		HttpServletRequest request = (HttpServletRequest) invocation.getInvocationContext();		HttpSession s = request.getSession(true);				if (s.getAttribute("Usuario") != null ) {						return invocation.invoke();		}		return "error";	}}