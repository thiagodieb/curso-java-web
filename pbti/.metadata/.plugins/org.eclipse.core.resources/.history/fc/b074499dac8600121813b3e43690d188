package interceptores;import javax.servlet.http.HttpSession;import org.apache.struts2.ServletActionContext;import com.opensymphony.xwork2.*;import com.opensymphony.xwork2.interceptor.Interceptor;public class LoginInterceptor implements Interceptor {	public void destroy() {	}	public void init() {	}	public String intercept(ActionInvocation invocation) throws Exception {		String result = "";		HttpSession s = ServletActionContext.getRequest().getSession();		if (s.getAttribute("Usuario") == null && 			!invocation.getAction().getClass().getName().equals("actions.LoginAction") ) {			result = "error";		}else{			result = invocation.invoke();		}				System.out.println( invocation.getAction().getClass().getName()+ ":" + result);				return result;	}}