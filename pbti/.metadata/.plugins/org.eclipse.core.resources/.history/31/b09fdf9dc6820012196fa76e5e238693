package beans;

import javax.persistence.*;

@Entity
public class Cursosss {
	@Id
	@GeneratedValue
	private int id;
	private String nomeDoCurso;
	private String tipo;
	private int horas;
	public int getId() {
		return id;
	}
	public void setId(int id) {
		this.id = id;
	}
	public String getNomeDoCurso() {
		return nomeDoCurso;
	}
	public void setNomeDoCurso(String nomeDoCurso) {
		this.nomeDoCurso = nomeDoCurso;
	}
	public String getTipo() {
		return tipo;
	}
	public void setTipo(String tipo) {
		this.tipo = tipo;
	}
	public int getHoras() {
		return horas;
	}
	public void setHoras(int horas) {
		this.horas = horas;
	}
	
	public Cursosss() {
		// TODO Auto-generated constructor stub
	}
	public Cursosss(int id, String nomeDoCurso, String tipo, int horas) {
		super();
		this.id = id;
		this.nomeDoCurso = nomeDoCurso;
		this.tipo = tipo;
		this.horas = horas;
	}
	
	

}
