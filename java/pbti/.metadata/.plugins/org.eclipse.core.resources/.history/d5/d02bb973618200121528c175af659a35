package action;

import org.apache.struts2.ServletActionContext;

public class Pessoa extends ServletActionContext{ 
 	/**
	 * 
	 */
	private static final long serialVersionUID = -4059800138488489308L;
	private String nome;
	private int idade;
	public String getNome() {
		return nome;
	}
	public void setNome(String nome) {
		this.nome = nome;
	}
	public int getIdade() {
		return idade;
	}
	public void setIdade(int idade) {
		this.idade = idade;
	} 
	
	public String cadastrar(){
		
		return "success";
	}
	
}
